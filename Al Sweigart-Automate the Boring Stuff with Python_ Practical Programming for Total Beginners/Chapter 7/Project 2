# don't know how to input letter in string
def my_strip(string = '', letter = ''):
    if letter == '' :
        string = re.compile("\S+").search(string).group()
        return string
    else:
        string = re.compile('[^'print(letter)']''+.*''[^'print(letter)']+).search(string).group()
        return string

re.compile([^a]+.*[^a]+).search('asada').group()
'sad'
