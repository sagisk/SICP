import openpyxl

wb = openpyxl.load_workbook('mine.xlsx')

for i in range(1, 8):
   for j in range(1, 8):
    wb.get_sheet_by_name('Shhet1').cell(i + 1, j + 1).value = i * j
    wb.get_sheet_by_name('Shhet1').cell(1, i + 1).value = i
    wb.get_sheet_by_name('Shhet1').cell(i + 1, 1).value = i

wb.save('mine.xlsx')
