(define (union-set s1 s2)
  (cond ((and (null? s1) (not (null? s2))) 
          s2) 
         ((and (not (null? s1)) (null? s2)) 
          s1) 
         ((element-of-set (car s1) s2)
               (union-set (cdr s1) s2))
               (else (cons (car s1) 
                     (union-set (cdr s1) s2)))))
