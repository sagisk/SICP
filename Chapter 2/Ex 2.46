(define (make-vect x y) (cons x y))
(define (xcor-vect x) (car x))
(define (ycor-vect y) (car (cdr y)))

(define (add-vect vec1 vec2)
    (make-vect (+ (xcor-vect vec1) (xcor-vect vec2)
             (ycor-vect vec1) (ycor-vect vec2))))
(define (scale-vect vec factor)
   (make-vect (* (xcor-vect vec1) factor) (* (xcor-vect vec1) factor)))
  
